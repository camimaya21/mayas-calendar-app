    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "i18next": "^19.9.1",
    "i18next-http-backend": "^1.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.8",
    "react-query": "^3.12.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
