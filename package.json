  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:cov": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint src/* --ext .js,.ts,.tsx --quiet --no-error-on-unmatched-pattern",
    "lint-fix": "eslint src/* --fix --ext .js,.ts,.tsx --quiet --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.ts\" "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "eslintIgnore": [
    "*.css",
    "*.png",
    "src/assets/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
